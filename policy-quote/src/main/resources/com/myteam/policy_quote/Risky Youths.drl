package com.myteam.policy_quote;

rule "Risky Youths"
    salience 4
    when
        //conditions
        $driver : Driver(age >= 18 && < 25, numberOfAccidents >= 1 || numberOfTickets >=2)
        $policy : Policy(policyType == "AUTO")
    then
        //actions
        modify($policy) {setPrice(700)};
        
end
