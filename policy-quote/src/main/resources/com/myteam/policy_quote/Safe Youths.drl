package com.myteam.policy_quote;

rule "SafeYouths"
    when
        //conditions
        $driver : Driver(age >= 18 && < 25, numberOfAccidents < 1, numberOfTickets <= 1)
        $policy : Policy(price == 0, policyType == "AUTO")
    then
        //actions
        modify($policy) {setPrice(450)};
        
end